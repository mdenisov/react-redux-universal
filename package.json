{
  "name": "react-redux-universal",
  "version": "0.0.1",
  "description": "Example of the universal app on react, redux, koa, etc.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf public/client",
    "compile": "export DEBUG=app:* PROJECT_PATH= NODE_ENV=production && npm run clean && npm run _compile:client && npm run _compile:server",
    "_compile:client": "webpack",
    "_compile:client:watch": "webpack --watch --watch-aggregate-timeout 100",
    "_compile:server": "webpack --config webpack.config-server.js",
    "_compile:server:watch": "webpack --config webpack.config-server.js --watch --watch-aggregate-timeout 100",
    "_start:server": "node bin/server",
    "_start:dev-server": "node bin/webpack-dev-server",
    "lint": "eslint src/**/*.js src/*.js server/*.js server/**/*.js config/*.js config/**/*.js build/*.js build/**/*.js",
    "dev": "export DEBUG=app:* PROJECT_PATH= NODE_HOST=localhost NODE_PORT=4000 && npm run clean && npm run _compile:client && npm run _compile:server && concurrent --kill-others \"npm run _compile:client:watch\" \"npm run _compile:server:watch\" \"npm run _start:server\"",
    "watch": "export DEBUG=app:* PROJECT_PATH= NODE_HOST=localhost NODE_PORT=4000 && npm run clean && npm run _compile:client -- --hmr && npm run _compile:server -- --hmr && concurrent --kill-others \"npm run _compile:server:watch -- --hmr\" \"npm run _start:dev-server -- --hmr\" \"npm run _start:server -- --hmr\"",
    "watch:logger": "export DEBUG=app:* PROJECT_PATH= NODE_HOST=localhost NODE_PORT=4000 && npm run clean && npm run _compile:client -- --hmr --no_debug --redux_logger && npm run _compile:server -- --hmr --no_debug --redux_logger && concurrent --kill-others \"npm run _compile:server:watch -- --hmr --no_debug --redux_logger\" \"npm run _start:dev-server -- --hmr --no_debug --redux_logger\" \"npm run _start:server -- --hmr --no_debug --redux_logger\"",
    "start": "npm run watch"
  },
  "author": "Aleksandr Efremov <eav@bystrobank.ru>",
  "license": "MIT",
  "engines": {
    "node": "4.2.3"
  },
  "dependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta9",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-syntax-export-extensions": "^6.3.13",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.5",
    "babel-plugin-transform-es2015-parameters": "^6.4.5",
    "babel-plugin-transform-es2015-spread": "^6.4.0",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-plugin-transform-regenerator": "^6.4.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "classnames": "^2.2.3",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.16.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "html-webpack-plugin": "^2.7.2",
    "imports-loader": "^0.6.5",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.8",
    "koa-favicon": "^1.2.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.2.3",
    "koa-send": "^3.1.1",
    "loader-utils": "^0.2.12",
    "node-fetch": "^1.3.3",
    "postcss-loader": "^0.8.0",
    "qs": "^5.2.0",
    "react": "^0.14.7",
    "react-addons-shallow-compare": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-helmet": "^2.3.1",
    "react-redux": "^4.2.1",
    "react-router": "^1.0.3",
    "redux": "^3.2.1",
    "redux-form": "^4.1.6",
    "redux-form-schema": "0.0.3",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "regenerator": "^0.8.42",
    "scroll-behavior": "^0.3.0",
    "serialize-javascript": "^1.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "whatwg-fetch": "^0.11.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "piping": "^0.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "rimraf": "^2.5.2",
    "webpack-dev-server": "^1.14.1"
  }
}
