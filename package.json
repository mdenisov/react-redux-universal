{
  "name": "react-redux-universal",
  "version": "0.1.0",
  "description": "Example of the universal app on react, redux, koa, etc.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf public/client",
    "compile": "export DEBUG=app:* PROJECT_PATH= NODE_ENV=production && npm run clean && npm run _compile:client && npm run _compile:server",
    "_compile:client": "webpack",
    "_compile:client:watch": "webpack --watch --watch-aggregate-timeout 100",
    "_compile:server": "webpack --config webpack.config-server.js",
    "_compile:server:watch": "webpack --config webpack.config-server.js --watch --watch-aggregate-timeout 100",
    "_start:server": "node bin/server",
    "_start:dev-server": "node bin/webpack-dev-server",
    "lint": "eslint src/**/*.js src/*.js server/*.js server/**/*.js config/*.js config/**/*.js build/*.js build/**/*.js",
    "lint:staged": "lint-staged",
    "dev": "export DEBUG=app:* PROJECT_PATH= && npm run clean && npm run _compile:client && npm run _compile:server && concurrently --kill-others \"npm run _compile:client:watch\" \"npm run _compile:server:watch\" \"npm run _start:server\"",
    "watch": "export DEBUG=app:* PROJECT_PATH= && npm run clean && npm run _compile:client -- --hmr && npm run _compile:server -- --hmr && concurrently --kill-others \"npm run _compile:server:watch -- --hmr\" \"npm run _start:dev-server -- --hmr\" \"npm run _start:server -- --hmr\"",
    "watch:logger": "export DEBUG=app:* PROJECT_PATH= && npm run clean && npm run _compile:client -- --hmr --no_debug --redux_logger && npm run _compile:server -- --hmr --no_debug --redux_logger && concurrently --kill-others \"npm run _compile:server:watch -- --hmr --no_debug --redux_logger\" \"npm run _start:dev-server -- --hmr --no_debug --redux_logger\" \"npm run _start:server -- --hmr --no_debug --redux_logger\"",
    "start": "npm run watch"
  },
  "lint-staged": {
    "eslint": "*.js",
    "stylelint": "*.css"
  },
  "pre-commit": "lint:staged",
  "author": "Aleksandr Efremov <eav@bystrobank.ru>",
  "license": "MIT",
  "engines": {
    "node": ">4.0.0 <5.0.0"
  },
  "dependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-syntax-export-extensions": "^6.3.13",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-plugin-transform-es2015-parameters": "^6.9.0",
    "babel-plugin-transform-es2015-spread": "^6.4.0",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
